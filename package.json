{
  "author": "glacambre",
  "dependencies": {
    "@types/jest": "25.2.1",
    "@types/msgpack-lite": "0.1.7",
    "@types/node": "13.13.1",
    "ace-builds": "^1.4.11",
    "awesome-typescript-loader": "5.2.1",
    "chromedriver": "81.0.0",
    "codemirror": "^5.53.0",
    "copy-webpack-plugin": "5.1.1",
    "geckodriver": "1.19.1",
    "jest": "25.4.0",
    "monaco-editor": "^0.20.0",
    "msgpack-lite": "0.1.26",
    "selenium-webdriver": "4.0.0-alpha.7",
    "sharp": "0.25.2",
    "ts-jest": "25.4.0",
    "tslint": "6.1.1",
    "typescript": "3.8.3",
    "web-ext": "4.1.0",
    "web-ext-types": "3.2.1",
    "webextension-polyfill": "0.6.0",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11"
  },
  "description": "Turn your browser into a Neovim GUI.",
  "keywords": [
    "chrome",
    "chromium",
    "firefox",
    "nvim",
    "vim",
    "webext",
    "webextension"
  ],
  "license": "GPL-3.0",
  "name": "Firenvim",
  "scripts": {
    "build": "tslint --fix --project . && webpack && web-ext build --source-dir target/firefox --artifacts-dir target/xpi --overwrite-dest",
    "clean": "rm -rf target",
    "install_manifests": "nvim --headless -u NORC -i NONE -n -c \":set rtp+=.\" -c \"call firenvim#install(1)\" -c \"quit\"",
    "test": "jest"
  },
  "jest": {
    "testRegex": "/tests/[^_].*\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "json",
      "js",
      "ts"
    ]
  },
  "version": "0.1.24"
}
