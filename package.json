{
  "author": "glacambre",
  "dependencies": {
    "@types/msgpack-lite": "0.1.6",
    "@types/node": "12.7.0",
    "@types/react": "16.8.25",
    "@types/react-dom": "16.8.5",
    "awesome-typescript-loader": "5.2.1",
    "copy-webpack-plugin": "5.0.4",
    "msgpack-lite": "0.1.26",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "tslint": "5.18.0",
    "tslint-sonarts": "1.9.0",
    "typescript": "3.5.3",
    "web-ext": "3.1.1",
    "web-ext-types": "3.2.1",
    "webextension-polyfill": "0.4.0",
    "webpack": "4.39.1",
    "webpack-cli": "3.3.6"
  },
  "description": "A webextension to turn Firefox into a Neovim client.",
  "keywords": [
    "webextension",
    "webext",
    "vim",
    "firefox"
  ],
  "license": "GPL-3.0",
  "name": "Firenvim",
  "scripts": {
    "build": "tslint --fix --project . && webpack && web-ext build --source-dir target/firefox --artifacts-dir target/xpi --overwrite-dest",
    "install": "nvim --headless -c ':set rtp+=.' -c 'call firenvim#install()' -c 'quit' || true",
    "clean": "rm -rf target"
  },
  "version": "0.1.7"
}
